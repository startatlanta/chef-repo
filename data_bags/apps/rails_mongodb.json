{
  "id": "app",
  "server_roles": [
    "app"
  ],
  "type": {
    "app": [
      "rails",
      "unicorn"
    ]
  },
  "database_master_role": [
    "database_master"
  ],
  "repository": "git://github.com/startatlanta/test-rails-mongodb.git",
  "revision": {
    "staging": "master",
    "production": "master"
  },
  "force": {
    "staging": "true",
    "production": "true"
  },
  "migrate": {
    "staging": "true",
    "production": "true"
  },
  "databases": {
    "staging": {
      "reconnect": "true",
      "encoding": "utf8",
      "adapter": "mongoid",
      "database": "app_staging",
      "port": "27017"
    },
    "production": {
      "reconnect": "true",
      "encoding": "utf8",
      "adapter": "mongoid",
      "database": "app_production",
      "port": "27017"
    }
  },
  "deploy_to": "/srv/app",
  "owner": "nobody",
  "group": "nogroup",
  "packages": {
    "curl":""
  },
  "gems": {
    "rails": "3.0.3",
    "bundler":"1.0.9"
  }
}